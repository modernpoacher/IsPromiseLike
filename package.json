{
  "name": "is-promise-like",
  "version": "0.1.6",
  "description": "Identifies a Promise or an object with a Promise-like interface",
  "main": "./index.js",
  "scripts": {
    "prepare": "npm run clean && cross-env NODE_ENV=production npm run build",
    "install": "npm run prepare",
    "build": "npm run babel",
    "build:watch": "npm run babel:watch",
    "clean": "rimraf lib",
    "babel": "babel src -d lib",
    "babel:watch": "npm run babel -- -w",
    "lint": "eslint *.js src test",
    "lint:fix": "npm run lint -- --fix",
    "test": "cross-env NODE_ENV=test mocha test --recursive -r @babel/register --verbose",
    "pre-commit": "npm run lint && npm test && npm run build && git add lib"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com/modernpoacher/is-promise-like.git"
  },
  "license": "MIT",
  "author": "Jonathan Perry for Modern Poacher Limited",
  "standard": {
    "parser": "babel-eslint"
  },
  "dependencies": {
    "cross-env": "^7.0.2",
    "debug": "^4.1.1",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/register": "^7.10.3",
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "chai": "^4.2.0",
    "eslint": "^7.3.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "mocha": "^8.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit"
    }
  }
}
